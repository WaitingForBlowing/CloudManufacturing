<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="forgetPass" prefHeight="340" prefWidth="600" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lcl.controller.ForgetPasswordController">
   <children>
      <VBox layoutX="1.0" prefHeight="340.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="9.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="102.0" text="账号：">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="account" minHeight="-Infinity" minWidth="-Infinity" prefHeight="24.0" prefWidth="242.0">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets left="90.0" top="80.0" />
               </padding>
            </HBox>
            <HBox prefHeight="9.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="102.0" text="确认邮箱：">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="email" minHeight="-Infinity" minWidth="-Infinity" prefHeight="24.0" prefWidth="242.0">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets left="90.0" />
               </padding>
            </HBox>
            <HBox prefHeight="9.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="102.0" text="验证码：">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="code" minHeight="-Infinity" minWidth="-Infinity" prefHeight="24.0" prefWidth="242.0">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Hyperlink maxHeight="-Infinity" minHeight="24.0" onMouseClicked="#send" prefHeight="24.0" prefWidth="105.0" text="发送验证码">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Hyperlink>
               </children>
               <padding>
                  <Insets left="90.0" />
               </padding>
            </HBox>
            <HBox prefHeight="9.0" prefWidth="600.0">
               <padding>
                  <Insets left="90.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#findPassword" text="找回密码">
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" text="重置">
                     <HBox.margin>
                        <Insets left="140.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
